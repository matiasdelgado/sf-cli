{
  "name": "sf-cli",
  "description": "SFDX wrapper",
  "version": "3.2.0",
  "author": "Matias Delgado",
  "bin": {
    "sf": "./bin/run"
  },
  "bugs": "https://github.com/matiasdelgado/sf-cli/issues",
  "dependencies": {
    "@oclif/core": "^4.1.0",
    "@oclif/plugin-autocomplete": "^3.2.14",
    "@oclif/plugin-help": "^6.2.19",
    "@oclif/plugin-not-found": "^3.2.31",
    "@oclif/plugin-version": "^2.2.18",
    "chalk": "^4.1.2",
    "inquirer": "^12.2.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.0",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.17.0",
    "eslint": "^8.56.0",
    "eslint-config-oclif": "^5.2.2",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-unicorn": "^56.0.1",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/matiasdelgado/sf-cli",
  "keywords": [
    "cli",
    "salesforce",
    "sfdx"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "additionalHelpFlags": [
      "-h"
    ],
    "additionalVersionFlags": [
      "-v"
    ],
    "commands": "./src/commands",
    "bin": "sf",
    "plugins": [
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-help",
      "@oclif/plugin-not-found",
      "@oclif/plugin-version"
    ]
  },
  "repository": "matiasdelgado/sf-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint .",
    "prepack": "oclif manifest && oclif readme",
    "test": "jest -- __tests__",
    "version": "oclif readme && git add README.md"
  }
}
